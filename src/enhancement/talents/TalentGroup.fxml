<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<TitledPane fx:id="pane" xmlns:fx="http://javafx.com/fxml" >
   <TableView fx:id="table" editable="true" fixedCellSize="28" >
      <styleClass>
         <String fx:value="remove-vertical-scrollbar" />
         <String fx:value="remove-horizontal-scrollbar" />
      </styleClass>
      <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/></columnResizePolicy>
      <placeholder>
         <Label minHeight="0" />
      </placeholder>
      <columns>
         <TableColumn fx:id="nameColumn" text="Talent" minWidth="100" resizable="false" prefWidth="200" editable="false" />
         <TableColumn fx:id="sesColumn" text="SEs" prefWidth="65" resizable="false" sortable="false" style="-fx-alignment: CENTER;" />
         <TableColumn fx:id="startColumn" text="Von" prefWidth="65" resizable="false" sortable="false" style="-fx-alignment: CENTER;" />
         <TableColumn fx:id="targetColumn" text="Auf" prefWidth="65" resizable="false" sortable="false" editable="true" style="-fx-alignment: CENTER;" />
         <TableColumn fx:id="methodColumn" text="Lernmethode" prefWidth="130" resizable="false" sortable="false" style="-fx-alignment: CENTER;" />
         <TableColumn fx:id="costColumn" text="Kosten" prefWidth="75" resizable="false" sortable="false" style="-fx-alignment: CENTER;" />
         <TableColumn fx:id="validColumn" minWidth="0" maxWidth="0" prefWidth="0" />
         <TableColumn fx:id="cheaperColumn" minWidth="0" maxWidth="0" prefWidth="0" />
      </columns>	
   </TableView>
</TitledPane>	